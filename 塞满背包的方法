#include<iostream>
#include<cstdio>
#include<cstring>
using namespace std;
int a[51],d[71][71];
/*d[i][j] i是背包容量，j是物品数 d方法数*/
int main()
{
    int n;
    scanf("%d",&n);
    memset(d,0,sizeof(d));
    for(int i=1; i<=n; i++)
    {
        scanf("%d",&a[i]);
        d[0][i]=0;
    }
    d[0][0]=1;
    for(int i=0; i<=70; i++)
        for(int j=1; j<=n; j++)
        {
            d[i][j]=d[i][j-1];
            if(a[j]<=i) d[i][j]+=d[i-a[j]][j-1];
            /*方法数为 不拿这一件+拿着一件前*/
        }
    printf("%d",d[70][n]);
    return 0;
}
